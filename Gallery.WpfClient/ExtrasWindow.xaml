<Window x:Class="Gallery.WpfClient.ExtrasWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Gallery.WpfClient" xmlns:local1="clr-namespace:Gallery.WpfClient.ViewModels"
        mc:Ignorable="d"
        Title="ExtrasWindow" Height="450" Width="800">
    <Window.DataContext>
        <local1:ExtrasWindowViewModel/>
    </Window.DataContext>
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="1*"/>
            <ColumnDefinition Width="2*"/>
        </Grid.ColumnDefinitions>
        <StackPanel Grid.Column="0">
            <Button Content="List G-mail users" Padding="10" Margin="10"/>
            <Button Content="Count paintings" Padding="10" Margin="10"/>
            <Button Content="Top 3 most expensive paintings" Padding="10" Margin="10"/>
            <Button Content="List better than average free exhibits" Padding="10" Margin="10"/>
            <Button Content="" Padding="10" Margin="10"/>
            

        </StackPanel>
    </Grid>
</Window>
